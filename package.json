{
  "name": "slideout",
  "repository": "git@github.com:mango/slideout.git",
  "description": "A touch slideout navigation menu for your mobile web apps.",
  "author": "Guille Paz <guille87paz@gmail.com>",
  "version": "2.0.0",
  "scripts": {
    "build": "webpack --mode development",
    "test": "npm run build && istanbul cover _mocha",
    "dist": "webpack --mode production && uglifyjs ./dist/slideout.js -m -o ./dist/slideout.min.js",
    "hint": "jshint index.js"
  },
  "dependencies": {
    "decouple": "^1.0.1",
    "jvent": "1.0.2"
  },
  "devDependencies": {
    "better-assert": "1.0.2",
    "coveralls": "2.11.15",
    "istanbul": "0.4.5",
    "jsdom": "9.8.3",
    "jshint": "2.9.4",
    "mocha": "3.1.2",
    "uglify-js": "2.7.5",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9"
  },
  "main": "index.js",
  "keywords": [
    "slideout",
    "offcanvas",
    "menu",
    "touch"
  ],
  "license": "MIT",
  "filename": "slideout.min.js",
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/Mango/slideout.git",
    "basePath": "dist",
    "files": [
      "*.js"
    ]
  }
}
